<item>
<title>[COLOR pink]DOCUMEN[/COLOR][COLOR red]TALES[/COLOR]</title>
<link>$doregex[makelist]</link>
 
<regex>
<name>makelist</name>
<listrepeat><![CDATA[
<SetViewMode>55</SetViewMode>
<title>[COLOR white][makelist.param2] [/COLOR]   </title>
<link>http:$doregex[link]</link>
<thumbnail>https://www.letrayletra.com/images/biografia.jpg</thumbnail>
]]></listrepeat>
<expres><![CDATA[h3.*?href="(.*?)".*?title="(.*?)"]]></expres>
<page>http://www.documaniatv.com/categorias-y-canales.html</page>
</regex>
 
<regex>
<name>link</name>
<expres>file:\s\"(.*?)"</expres>
<page>http://cnubis.com/plugins/mediaplayer/embeder/_embed2.php?u=$doregex[id2]</page>
</regex>
<regex>
<name>id2</name>
<expres>embed2\.php\?u=(.*?)"</expres>
<page>[makelist.param1]</page>
</regex>
<thumbnail>https://www.letrayletra.com/images/biografia.jpg</thumbnail>
</item>



