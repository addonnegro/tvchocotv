<regex>
<name>lista</name>
  <listrepeat><![CDATA[
         <SetViewMode>55</SetViewMode>
         <title>$pyFunction:'[lista.param3]'.replace(".mp4", "  [COLOR blue][I]tvchopo[/I][/COLOR]").replace(".avi", "  [COLOR blue][I]tvchopo[/I][/COLOR]").replace(".mkv", "  [COLOR red][I][/I][/COLOR]")</title>
        <link>$doregex[getUrl]</link>
